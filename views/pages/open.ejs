<%- include('../template/header') -%>
<%- include('../template/navbar') -%>


<div>
    <div class="container pt-4">
        <h1 class="text-center" style='margin-top:3em'>
            <%= output.newsPaper %>
        </h1>
        <h2 class="text-center">
            <%= output.headline %>
        </h2>
        <div class="row">
            <div class="col-md-4">
                <h5>
                    <b>Date: </b><%= output.date.toISOString().split('T')[0] %>
                </h5>
            </div>
            <div class="col-md-4">
                <h5>
                    <b>District: </b><%= output.district %>
                </h5>
            
            </div>
            <div class="col-md-4">
                <h5>
                    <b>Page Number: </b><%= output.pageNumber %>
                </h5>
            </div>
        </div>
        
        
        <div class="text-center">
            <img class="img-fluid" src="<%= output.image %>" alt=""> 
        </div>
        
        
    </div>
</div>




<%- include('../template/footer') -%>
